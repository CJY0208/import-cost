    "mocha": "^2.3.3",
    "tslint": "^5.5.0",
    "tslint-config-wix": "^1.0.25",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-types": "^6.25.0",
    "babili-webpack-plugin": "^0.1.2",
    "babylon": "^6.17.4",
    "css-loader": "^0.28.4",
    "filesize": "^3.5.10",
    "memory-fs": "^0.4.1",
