  "description": "Display import/require package size in the editor",
  "version": "1.0.15",
  "publisher": "yairhaimo",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": ["Other"],
  "activationEvents": ["*"],
  "keywords": ["import", "require", "size", "weight", "webpack"],
  "main": "./out/src/extension",
