    "mocha": "^2.3.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-types": "^6.25.0",
    "babili-webpack-plugin": "^0.1.2",
    "babylon": "^6.17.4",
    "memory-fs": "^0.4.1",
    "pkg-dir": "^2.0.0",
    "tmp": "0.0.31",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "worker-farm": "^1.4.1"
