  "description": "Display import/require package size in the editor",
  "version": "1.1.6",
  "publisher": "wix",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "import",
    "require",
    "size",
